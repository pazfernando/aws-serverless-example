# Example Terraform variables for dev environment
# Copy this file to terraform.tfvars and customize the values

# Project name - used as prefix for all resources
project_name = "fp-aws-serverless-sample-1"

# AWS region where resources will be deployed
region = "us-east-1"

# Tags to apply to all resources
tags = {
  Project     = "fp-aws-serverless-sample-1"
  Environment = "dev"
  ManagedBy   = "terraform"
  Owner       = "your-team-name"
}

# CORS allowed origins for API Gateway
# Use ["*"] for development, specific domains for production
cors_allow_origins = ["*"]
# cors_allow_origins = ["https://yourdomain.com", "https://www.yourdomain.com"]
